{
  "name": "sfdxdatadicgen",
  "description": "Sdfx plugin that generates Data Dictionary from an org",
  "version": "0.1.3",
  "author": "Marcelo Marsson @marsson",
  "bugs": "https://github.com/marsson/dataDicGen/issues",
  "dependencies": {
    "@oclif/command": "^1.8.20",
    "@oclif/config": "^1.18.6",
    "@oclif/errors": "^1.3.6",
    "@salesforce/command": "^5.3.9",
    "@salesforce/core": "^3",
    "@sinonjs/fake-timers": "^10.0.0",
    "@types/sinonjs__fake-timers": "^8.1.2",
    "bytes": "^3.1.2",
    "excel4node": "^1.8.0",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/plugin-help": "^5.1.19",
    "@oclif/test": "^2.2.12",
    "@salesforce/dev-config": "3.1.0",
    "@types/chai": "^4.3.4",
    "@types/jsforce": "^1.11.0",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.7",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.0",
    "@types/node": "^18.11.9",
    "typescript": "^4.9.3",
    "tsutils": "^3.21.0",
    "tslint": "^6.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/marsson/dataDicGen",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "marsson/dataDicGen",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
